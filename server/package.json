{
  "name": "umbrella-server",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev:prod": "nodemon  --es-module-specifier-resolution=node dist/index.js",
    "start": "cross-env NODE_ENV=development concurrently -k -n 'build,server,admin' \"tsc -w\" \"npm run dev:prod\" \"npm run dev:admin\"",
    "build:admin": "npm --prefix ./admin run build",
    "dev:admin": "cd admin && npm run dev",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "prisma:migrate:dev": "dotenv -e .env.dev npx prisma migrate dev --create-only",
    "prisma:migrate:reset": "dotenv -e .env.dev npx prisma migrate reset",
    "prisma:generate": "dotenv -e .env.dev npx prisma generate --create-only",
    "prisma:dev:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.359.0",
    "@prisma/client": "4.10.1",
    "aws-sdk": "^2.1401.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "4.18.2",
    "express-rate-limit": "^6.7.0",
    "googleapis": "^120.0.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.3",
    "pug": "^3.0.2",
    "redis": "^4.6.7",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "4.17.17",
    "@types/helmet": "^0.0.45",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "18.14.1",
    "@types/nodemailer": "^6.4.8",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.8.8",
    "prisma": "4.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  }
}
